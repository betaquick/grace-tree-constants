{
  "name": "@betaquick/grace-tree-constants",
  "version": "1.0.11",
  "description": "a module to manage constants across grace tree projects",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/betaquickdevelopers/grace-tree-constants.git"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register -r source-map-support/register test/**/*.spec.ts",
    "test-coverage": "nyc mocha -r ts-node/register -r source-map-support/register test/**/*.spec.ts"
  },
  "keywords": [
    "constants",
    "grace-tree",
    "beta-quick"
  ],
  "author": "Jimoh Damilola",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "homepage": "https://bitbucket.org/betaquickdevelopers/grace-tree-constants#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  },
  "dependencies": {}
}
